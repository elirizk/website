---
const { id, path } = Astro.props;

// export const prerender = true;
---

<embed id={id} src={path} type="text/html" />

<style>
    embed {
        width: 100%;
        height: 395px;
        margin: 0 0 2em 0;
        border-radius: 0.5em;
        padding: 0;
        box-shadow: var(--box-shadow); ;
    }
</style>